
partial default alphanumeric_keys
xkb_symbols "basic" {
    include "fi"
    include "us"
    name[Group1] = "FI (US keyboard with altgr combinations)";

    key <AC01> { [ a, A, adiaeresis, Adiaeresis ] };
    key <AD07> { [ u, U, udiaeresis, Udiaeresis ] };
    key <AD09> { [ o, O, odiaeresis, Odiaeresis ] };
};

